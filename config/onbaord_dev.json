{
    "schemaVersion": "1.0.0",
    "class": "Device",
    "async": true,
    "label": "my BIG-IP declaration for declarative onboarding",
    "Common": {
        "class": "Tenant",
        "hostname": "bigip1.example.com",
        "admin": {
            "class": "User",
            "userType": "regular",
            "password": "{{{ ADMIN_PASS }}}",
            "keys": ["{{{ SSH_KEYS }}}"],
            "shell": "bash"
        },
        "{{{ USER_NAME }}}": {
            "class": "User",
            "userType": "regular",
            "password": "{{{ ADMIN_PASS }}}",
            "keys": ["{{{ SSH_KEYS }}}"],
            "shell": "bash",
            "partitionAccess": {
                "all-partitions": {
                    "role": "admin"
                }
            }
        }
    }
}
